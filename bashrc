# for emacs
alias emacs='emacs -nw'
alias e='emacs -nw'
alias ec="emacs -l ~/.emacs.d/emacs-plugins/my/c-settings.el"
alias ep="emacs -l ~/.emacs.d/emacs-plugins/my/pl-settings.el"
alias eo="emacs -l ~/.emacs.d/emacs-plugins/my/org-settings.el"
alias em="emacs -l ~/.emacs.d/emacs-plugins/my/muse-settings.el"
alias el="emacs -l ~/.emacs.d/emacs-plugins/my/clisp-settings.el"
